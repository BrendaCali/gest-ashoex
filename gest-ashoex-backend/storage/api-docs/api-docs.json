{
    "openapi": "3.0.0",
    "info": {
        "title": "API Health Check",
        "description": "API para verificar el estado de salud del sistema",
        "version": "1.0.0"
    },
    "paths": {
        "/api/carreras/{id}": {
            "delete": {
                "tags": [
                    "Carrera"
                ],
                "summary": "Elimina una carrera por el id que se tiene ",
                "description": "Devuelve el estado de salud del sistema",
                "operationId": "cf5360ba43551c82c0a1d58246d97560",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la carrera",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Eliminado con exito",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "type": "object"
                                            },
                                            "example": "[]"
                                        },
                                        "error": {
                                            "type": "array",
                                            "items": {
                                                "type": "object"
                                            },
                                            "example": "[]"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Operacion exitosa"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Error al eliminar una carrera",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "type": "object"
                                            },
                                            "example": "[]"
                                        },
                                        "error": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "code": {
                                                        "type": "integer",
                                                        "example": "422"
                                                    },
                                                    "detail": {
                                                        "type": "string",
                                                        "example": "el id proporcionado no esta relacionado con una carrera"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Error"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/health": {
            "get": {
                "tags": [
                    "Health Check"
                ],
                "summary": "Verifica el estado de la API y la conexión a la base de datos",
                "description": "Devuelve el estado de salud del sistema",
                "operationId": "7134f626524ee8957bd6315b959ed4e1",
                "responses": {
                    "200": {
                        "description": "Estado de salud correcto",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "healthy"
                                        },
                                        "database": {
                                            "type": "string",
                                            "example": "Connected"
                                        },
                                        "timestamp": {
                                            "type": "string",
                                            "format": "date-time"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Error en la conexión a la base de datos"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Carrera",
            "description": "Carrera"
        },
        {
            "name": "Health Check",
            "description": "Health Check"
        }
    ]
}